  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build-linux",
    "build-mac": "electron-packager ./ BananaBreak --platform=darwin --arch=x64 --overwrite --icon=img/icon.icns --extend-info info.plist",
